            "AwesIO\\Navigator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always --configuration phpunit.xml.dist --debug"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwesIO\\Navigator\\NavigatorServiceProvider"
            ],
            "aliases": {
